{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "sweetalert2@10.12.5",
      "destination": "wwwroot/sweetalert2/",
      "files": [
        "dist/sweetalert2.min.css",
        "dist/sweetalert2.min.js"
      ]
    }
  ]
}